<p id="notice"><%= notice %></p>

<h1>Programs</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Library</th>
      <th>Source file</th>
      <th>Code</th>
      <th>Is sftp</th>
      <th>Is ready</th>
      <th>Transfers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.member %></td>
        <td><%= program.library_id %></td>
        <td><%= program.source_file_id %></td>
        <td><%= program.code_id %></td>
        <td><%= program.is_sftp %></td>
        <td><%= program.is_ready %></td>
        <td><%= program.transfers %></td>
        <td><%= link_to 'Show', program %></td>
        <td><%= link_to 'Edit', edit_program_path(program) %></td>
        <td><%= link_to 'Destroy', program, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program', new_program_path %>
