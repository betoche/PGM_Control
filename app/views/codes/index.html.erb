<p id="notice"><%= notice %></p>

<h1>Codes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Operation</th>
      <th>Description</th>
      <th>Input</th>
      <th>Output</th>
      <th>Home dir</th>
      <th>Daily</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @codes.each do |code| %>
      <tr>
        <td><%= code.code %></td>
        <td><%= code.operation %></td>
        <td><%= code.description %></td>
        <td><%= code.input %></td>
        <td><%= code.output %></td>
        <td><%= code.home_dir %></td>
        <td><%= code.daily %></td>
        <td><%= link_to 'Show', code %></td>
        <td><%= link_to 'Edit', edit_code_path(code) %></td>
        <td><%= link_to 'Destroy', code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code', new_code_path %>
